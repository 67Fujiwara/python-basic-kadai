        "\n",
        "var = random.randint(0,15)\n",
        "\n",
        "if(var % 3 == 0 and var %5 != 0 ):\n",
        "  print(\"Fizz\")\n",
        "elif(var % 5 == 0 and var %3 != 0):\n",
        "  print(\"Buzz\")\n",
        "elif(var %3 == 0 and var %5 == 0):\n",
        "  print(\"FizzBuzz\")\n",
        "else:\n",
        "  print(var)"
