      "source": [
        "def is_purchased(value: int, taxrate: int)-> int:\n",
        "  tax_included = value * ((100 + taxrate) / 100)\n",
        "\n",
        "  return int(tax_included)\n",
        "\n",
        "total_value = is_purchased(200, 10)\n",
        "print(f\"税込み{total_value}円になります\")"
      ]
