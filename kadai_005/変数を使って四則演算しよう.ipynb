        "#上辺：10cm\n",
        "#下辺：20cm\n",
        "#高さ：5cm\n",
        "#公式：台形の面積 = (上辺＋下辺) * 高さ / 2\n",
        "\n",
        "upper_side = 10\n",
        "lower_side = 20\n",
        "hight_side = 5\n",
        "\n",
        "trapezoid = (upper_side + lower_side) * hight_side / 2\n",
        "print(f\"{trapezoid}CM\")"
      ]
